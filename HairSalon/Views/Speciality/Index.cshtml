<h1>List of Specialitys</h1>
<p><a href="/stylist/new">Add Stylist</a></p>
<p><a href="/stylist">List of Stylist</a></p>
<p><a href="/specialitys/new">Add Speciality</a></p>
<p><a href="/specialitys">List of Speciality</a></p>

<table>
  <tr>
    <th>Speciality </th>
    <th>Update</th>
    <th>Delete</th>
  </tr>
  @foreach(var speciality in Model)
  {
    <tr>
      <td><a href="/specialitys/@speciality.GetSpecialityId()">@speciality.GetSpecialityName()</a></td>
      <td><a href="/specialitys/@speciality.GetSpecialityId()/update">Edit</a></td>
      <td><a href="/specialitys/@speciality.GetSpecialityId()/delete">Delete</a></td>

    </tr>
  }
</table>
<p><a href="/specialitys/new">Add a New Speciality</a></p>
<p><a href="/">Back to Home</a></p>
